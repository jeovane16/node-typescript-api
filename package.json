{
  "name": "node-typescript-api",
  "version": "1.0.0",
  "description": "Typescript NodeJs API",
  "main": "index.js",
  "author": "Jeovane Araujo <jeovanearaujo16@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "yarn lint && yarn style:check && yarn test:unit && yarn test:functional",
    "test:functional": "dotenv -e .env -- jest --projects ./test --runInBand",
    "test:unit": "dotenv -e .env -- jest",
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "dotenv -e .env -- ts-node-dev --transpile-only src/index.ts",
    "dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "eslint ./src ./test --ext .ts --fix"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^0.0.41",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.33",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "nock": "^13.2.7",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "config": "^3.3.7",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.3.6"
  }
}
